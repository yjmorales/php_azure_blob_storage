{
  "name": "yjmorales/php_azure_blob_storage",
  "license": "MIT",
  "type": "project",
  "description": "Upload images to Azure Blob Storage using PHP",
  "authors": [
    {
      "name": "Yenier Jimenez",
      "email": "yjmorales86@gmail.com",
      "homepage": "http://yenierjimenez.com",
      "role": "Web Developer"
    }
  ],
  "version": "0.1.0",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "composer/package-versions-deprecated": "1.11.99.1",
    "doctrine/annotations": "^1.13",
    "paragonie/sodium_compat": "^1.14",
    "phpdocumentor/reflection-docblock": "^5.3",
    "sendgrid/sendgrid": "^7.9",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/asset": "5.0.*",
    "symfony/cache": "5.0.*",
    "symfony/config": "5.0.*",
    "symfony/console": "5.0.*",
    "symfony/dependency-injection": "5.0.*",
    "symfony/dotenv": "5.0.*",
    "symfony/expression-language": "5.0.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.0.*",
    "symfony/http-client": "5.0.*",
    "symfony/intl": "5.0.*",
    "symfony/notifier": "5.0.*",
    "symfony/process": "5.0.*",
    "symfony/property-access": "5.0.*",
    "symfony/property-info": "5.0.*",
    "symfony/proxy-manager-bridge": "5.0.*",
    "symfony/security-csrf": "5.0.*",
    "symfony/serializer": "5.0.*",
    "symfony/string": "5.0.*",
    "symfony/twig-bundle": "5.0.*",
    "symfony/validator": "5.0.*",
    "symfony/var-dumper": "5.0.*",
    "symfony/web-link": "5.0.*",
    "symfony/yaml": "5.0.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "symfony/debug-bundle": "5.0.*",
    "symfony/stopwatch": "5.0.*",
    "symfony/web-profiler-bundle": "5.0.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.0.*"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
